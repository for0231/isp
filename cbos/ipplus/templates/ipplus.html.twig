{#
/**
 * @file ipplus.html.twig
 * Default theme implementation to present IP+ data.
 *
 * This template is used when viewing IP+ pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_ipplus()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('ipplus') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
